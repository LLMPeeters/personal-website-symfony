{% extends 'admin/base.html.twig' %}

{% block title %}Browse progress widgets{% endblock %}

{% block main %}
	<h1>Progress widgets</h1>
	
	<section class="list-browse">
		{% for widget in widgets %}
			{% include 'admin/widgets/progress_widget_bread/readComponent.html.twig' with {'widget': widget, 'headingStartsAtNum': 2} %}
		{% else %}
			<p>None!</p>
		{% endfor %}
	</section>
{% endblock %}
